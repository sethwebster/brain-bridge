{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "A scalable boilerplate for express.js and typescript with prisma config",
  "main": "index.ts",
  "scripts": {
    "sync-prisma": "npx prisma db pull && npx prisma generate",
    "build": "npx prisma generate && tsc",
    "start": "nodemon ./dist/index.js",
    "dev": "npx prisma migrate deploy && npx prisma db pull && nodemon --exec ts-node -- ./src/index.ts",
    "prod": "npx prisma db pull && npm run build && npm run start",
    "clean": "rm -rf node_modules coverage dist .nyc_output",
    "lint": "tslint -t stylish --project \"tsconfig.json\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabianferno/express-typescript-boilerplate.git"
  },
  "author": "Fabian Ferno",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fabianferno/express-typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/fabianferno/express-typescript-boilerplate#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "3.341.0",
    "@aws-sdk/s3-request-presigner": "3.341.0",
    "@huggingface/inference": "1.8.0",
    "@prisma/client": "4.15.0",
    "@types/jsdom": "21.1.1",
    "@types/turndown": "5.0.1",
    "@zilliz/milvus2-sdk-node": "2.2.7",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "helmet": "^4.6.0",
    "hnswlib-node": "1.4.2",
    "http-status": "^1.2.0",
    "inference": "^2.0.59",
    "jsdom": "22.1.0",
    "jwks-rsa": "^2.0.5",
    "langchain": "0.0.91",
    "morgan": "^1.10.0",
    "ms": "2.1.3",
    "njwt": "2.0.0",
    "nocache": "^3.0.1",
    "openai": "3.2.1",
    "pdfjs-dist": "3.7.107",
    "pickle": "0.2.0",
    "socket.io": "4.6.2",
    "tiny-invariant": "1.3.1",
    "turndown": "7.1.2",
    "typeorm": "0.3.16"
  },
  "devDependencies": {
    "@pinecone-database/pinecone": "0.1.6",
    "@supabase/supabase-js": "2.24.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/node": "^20.2.5",
    "chromadb": "1.5.2",
    "cohere-ai": "5.1.0",
    "morgan": "^1.9.1",
    "nodemon": "2.0.15",
    "prisma": "4.15.0",
    "ts-node": "^10.4.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "5.0.4"
  },
  "nodemonConfig": {
    "ext": "js,json,ejs,jpg,png,gif,svg,css,html,ts,env",
    "watch": [
      "src",
      "dist"
    ]
  },
  "optionalDependencies": {
    "fsevents": "2.3.2"
  }
}
