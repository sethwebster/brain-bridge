{
  "name": "photography-tutor",
  "version": "0.0.1",
  "description": "A chatbot for photography",
  "main": "/build/server/index.js",
  "scripts": {
    "sync-prisma": "mkdir -p ./prisma && cp -rf ../brain-bridge-t3/prisma/* ./prisma/",
    "build": "rm -rf ./build && mkdir -p ./build && npx prisma generate && cp -rf ./node_modules/.prisma/client ./build/prisma && npx tsc && cp -rf ./public ./build/server/public",
    "train": "node ./build/lib/training/initialize-store.js",
    "train:interactive": "node ./build/lib/training/interactive.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:server": "node ./build/server/index.js",
    "start": "node ./build/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon --watch ./build/server --exec node ./build/server/index.js\"",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "openai",
    "langchain",
    "llm"
  ],
  "author": "Seth Webster",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.1.0",
    "@types/redis": "^4.0.11",
    "@types/turndown": "^5.0.1",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.341.0",
    "@aws-sdk/s3-request-presigner": "^3.341.0",
    "@prisma/client": "^4.14.1",
    "@types/axios": "^0.14.0",
    "@types/glob": "^8.1.0",
    "@types/node-fetch": "^2.6.3",
    "@types/prompt-sync": "^4.2.0",
    "@types/rimraf": "^4.0.5",
    "aws-sdk": "^2.1386.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "glob": "^10.2.2",
    "hnswlib-node": "^1.4.2",
    "jsdom": "^22.0.0",
    "langchain": "^0.0.71",
    "node-fetch": "^3.3.1",
    "openai": "^3.2.1",
    "pickle": "^0.2.0",
    "prompt-sync": "^4.2.0",
    "redis": "^4.6.6",
    "rimraf": "^5.0.0",
    "tiny-invariant": "^1.3.1",
    "turndown": "^7.1.2"
  }
}
